{
	"name": "birthdayy-discord-bot",
	"version": "0.1.0",
	"main": "dist/index.js",
	"author": "@chillihero",
	"dependencies": {
		"@devtomio/plugin-botlist": "^1.3.0",
		"@discordjs/builders": "^1.5.0",
		"@sapphire/cron": "^1.1.0",
		"@sapphire/decorators": "^6.0.0",
		"@sapphire/discord.js-utilities": "^6.0.4",
		"@sapphire/fetch": "^2.4.1",
		"@sapphire/framework": "^4.2.2",
		"@sapphire/plugin-api": "^5.0.1",
		"@sapphire/plugin-editable-commands": "^3.0.0",
		"@sapphire/plugin-hmr": "^2.0.0",
		"@sapphire/plugin-i18next": "^5.0.1",
		"@sapphire/plugin-logger": "^3.0.1",
		"@sapphire/plugin-scheduled-tasks": "^6.0.0",
		"@sapphire/plugin-subcommands": "^4.0.0",
		"@sapphire/stopwatch": "^1.5.0",
		"@sapphire/time-utilities": "^1.7.9",
		"@sapphire/type": "^2.3.0",
		"@sapphire/utilities": "^3.11.0",
		"@types/node-cron": "^3.0.7",
		"bullmq": "^3.10.1",
		"colorette": "^2.0.19",
		"discord-api-types": "^0.37.36",
		"discord.js": "^14.8.0",
		"lib": "^5.0.1",
		"mysql2": "^3.2.0",
		"node-cron": "^3.0.2",
		"pm2": "^5.2.2",
		"reflect-metadata": "^0.1.13",
		"sequelize": "^6.29.3"
	},
	"devDependencies": {
		"@sapphire/eslint-config": "^4.3.8",
		"@sapphire/prettier-config": "^1.4.5",
		"@sapphire/ts-config": "^3.3.4",
		"@types/node": "^18.15.3",
		"@types/ws": "^8.5.4",
		"@typescript-eslint/eslint-plugin": "^5.55.0",
		"@typescript-eslint/parser": "^5.55.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.7.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-tsdoc": "^0.2.17",
		"npm-run-all": "^4.1.5",
		"prettier": "^2.8.4",
		"tsc-watch": "^6.0.0",
		"typescript": "^4.9.5"
	},
	"scripts": {
		"build": "tsc",
		"watch": "tsc -w",
		"start": "doppler run -- yarn node --enable-source-maps dist/index.js",
		"dev": "doppler run run-p watch start",
		"pm2-dev": "yarn build && yarn pm2-start --max-memory-restart=2G --watch && pm2 monit",
		"pm2-start": "doppler run pm2 start dist/index.js --name BirthdayyBot",
		"pm2-stop": "pm2 stop BirthdayyBot",
		"pm2-restart": "yarn build && doppler run pm2 restart BirthdayyBot && pm2 monit",
		"pm2-monit": "pm2 monit",
		"watch:start": "tsc-watch --onSuccess \"yarn start\"",
		"build:start": "tsc && yarn start",
		"format": "prettier --write \"src/**/*.ts\"",
		"lint": "eslint src --ext .ts"
	},
	"prettier": "@sapphire/prettier-config",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/BirthdayyBot/BirthdayyBotSapphire.git"
	}
}
