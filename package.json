{
  "name": "birthdayy-discord-bot",
  "version": "0.2.3",
  "description": "Birthday Discord Bot built on Sapphire Framework",
  "author": "@chillihero",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/commands": "./dist/lib/commands/index.js",
    "#lib/embeds": "./dist/lib/embeds/index.js",
    "#lib/database": "./dist/lib/database/index.js",
    "#lib/discord": "./dist/lib/discord/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/setup": "./dist/lib/setup/index.js",
    "#lib/types": "./dist/lib/types/index.js",
    "#root/*": "./dist/*.js",
    "#languages": "./dist/languages/index.js",
    "#utils": "./dist/lib/utils/index.js",
    "#utils/common": "./dist/lib/utils/common/index.js",
    "#utils/functions": "./dist/lib/utils/functions/index.js",
    "#utils/birthday": "./dist/lib/utils/birthday/index.js"
  },
  "scripts": {
    "start": "docker compose -f ./docker/docker-compose.yml up -d",
    "down": "docker compose -f ./docker/docker-compose.yml down",
    "build": "docker compose -f ./docker/docker-compose.yml build",
    "logs": "docker compose -f ./docker/docker-compose.yml logs -f",
    "shell": "docker exec -it birthdayy_bot bash",
    "doppler": "DOPPLER_TOKEN=\"$(doppler configs tokens create dev --plain --max-age 1m)\"",
    "dev": "yarn run doppler \\docker compose -f ./docker/docker-compose.dev.yml up",
    "dev:legacy": "doppler run -- yarn node --enable-source-maps dist/index.js",
    "dev:watch": "tsc-watch -b src --onSuccess \"yarn run dev:legacy\"",
    "dev:build": "docker compose -f ./docker/docker-compose.dev.yml build",
    "dev:down": "docker compose -f ./docker/docker-compose.dev.yml down",
    "dev:shell": "docker exec -it birthdayy-dev bash",
    "prepare": "husky install",
    "lint:fix": "eslint --fix --ext .ts,.js ./src",
    "clean": "node scripts/clean.mjs",
    "lint": "eslint --ext .ts,.js ./src",
    "dev:migrate": "docker exec -it birthdayy-dev npx prisma migrate dev",
    "dev:generate": "docker exec -it birthdayy-dev npx prisma generate",
    "db:migrate": "docker exec -it birthdayy npx prisma migrate deploy",
    "db:generate": "docker exec -it birthdayy npx prisma generate"
  },
  "dependencies": {
    "@devtomio/plugin-botlist": "^1.3.0",
    "@kaname-png/plugin-subcommands-advanced": "^2.2.3",
    "@prisma/client": "^5.2.0",
    "@sapphire/cron": "^1.1.1",
    "@sapphire/decorators": "^6.0.2",
    "@sapphire/discord.js-utilities": "^7.0.1",
    "@sapphire/eslint-plugin-result": "^2.0.0",
    "@sapphire/fetch": "^2.4.1",
    "@sapphire/framework": "^4.5.3",
    "@sapphire/plugin-api": "^5.1.1",
    "@sapphire/plugin-editable-commands": "^3.0.3",
    "@sapphire/plugin-hmr": "^2.0.2",
    "@sapphire/plugin-logger": "^3.0.6",
    "@sapphire/plugin-scheduled-tasks": "^7.1.2",
    "@sapphire/plugin-subcommands": "^4.2.0",
    "@sapphire/plugin-utilities-store": "^1.0.2",
    "@sapphire/result": "^2.6.4",
    "@sapphire/shapeshift": "^3.9.2",
    "@sapphire/stopwatch": "^1.5.0",
    "@sapphire/time-utilities": "^1.7.10",
    "@sapphire/type": "^2.4.0",
    "@sapphire/utilities": "^3.13.0",
    "@sentry/integrations": "^7.64.0",
    "@sentry/node": "^7.64.0",
    "@sentry/tracing": "^7.64.0",
    "@skyra/env-utilities": "^1.2.1",
    "bullmq": "^4.8.0",
    "colorette": "^2.0.20",
    "dayjs": "^1.11.9",
    "discord-api-types": "^0.37.54",
    "discord.js": "^14.13.0",
    "prisma": "^5.2.0",
    "reflect-metadata": "^0.1.13"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@sapphire/cli": "^1.6.1",
    "@sapphire/eslint-config": "^5.0.1",
    "@sapphire/plugin-i18next": "^5.0.5",
    "@sapphire/ts-config": "^4.0.1",
    "@types/node": "^20.5.4",
    "@types/ws": "^8.5.5",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-tsdoc": "^0.2.17",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.2",
    "prettier-plugin-packagejson": "^2.4.5",
    "prettier-plugin-prisma": "^5.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": "18.17.x",
    "npm": "^9.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BirthdayyBot/BirthdayyBotSapphire.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "packageManager": "yarn@3.6.3",
  "volta": {
    "node": "18.17.0",
    "yarn": "4.0.0-rc.50"
  },
  "resolutions": {
    "@devtomio/plugin-botlist@^1.3.0": "patch:@devtomio/plugin-botlist@npm%3A1.3.0#./.yarn/patches/@devtomio-plugin-botlist-npm-1.3.0-56fce49f49.patch"
  }
}
