{
  "name": "birthdayy",
  "version": "0.2.3",
  "description": "Birthday Discord Bot built on Sapphire Framework",
  "author": "@chillihero",
  "license": "Apache-2.0",
  "main": "./dist/index.js",
  "type": "module",
  "imports": {
    "#lib/*": "./dist/lib/*.js",
    "#lib/commands": "./dist/lib/commands/index.js",
    "#lib/embeds": "./dist/lib/embeds/index.js",
    "#lib/database": "./dist/lib/database/index.js",
    "#lib/discord": "./dist/lib/discord/index.js",
    "#lib/structures": "./dist/lib/structures/index.js",
    "#lib/setup": "./dist/lib/setup/index.js",
    "#lib/types": "./dist/lib/types/index.js",
    "#root/*": "./dist/*.js",
    "#languages": "./dist/languages/index.js",
    "#utils": "./dist/lib/utils/index.js",
    "#utils/*": "./dist/lib/utils/*.js",
    "#utils/common": "./dist/lib/utils/common/index.js",
    "#utils/functions": "./dist/lib/utils/functions/index.js",
    "#utils/birthday": "./dist/lib/utils/birthday/index.js"
  },
  "scripts": {
    "build": "tsc -b src",
    "dev": "yarn build && yarn start",
    "watch": "tsc -b src -w",
    "watch:start": "tsc-watch -b src --onSuccess \"yarn start\"",
    "clean": "node scripts/clean.mjs",
    "start": "doppler run -- node --enable-source-maps dist/index.js",
    "lint": "eslint --fix --ext ts src tests",
    "format": "prettier --write --log-level=warn \"{src,tests}/**/*.{js,ts,json}\"",
    "test": "eslint --ext .ts src",
    "update": "yarn upgrade-interactive",
    "prisma:generate": "npx prisma generate",
    "prisma:migrate": "npx prisma migrate dev"
  },
  "dependencies": {
    "@influxdata/influxdb-client": "^1.33.2",
    "@prisma/client": "^5.9.1",
    "@sapphire/async-queue": "^1.5.2",
    "@sapphire/decorators": "^6.0.4",
    "@sapphire/discord.js-utilities": "^7.1.6",
    "@sapphire/duration": "^1.1.2",
    "@sapphire/fetch": "^3.0.2",
    "@sapphire/framework": "^5.0.7",
    "@sapphire/plugin-api": "^6.1.1",
    "@sapphire/plugin-editable-commands": "^4.0.2",
    "@sapphire/plugin-hmr": "^3.0.1",
    "@sapphire/plugin-i18next": "^7.1.1",
    "@sapphire/plugin-logger": "^4.0.2",
    "@sapphire/plugin-scheduled-tasks": "^10.0.1",
    "@sapphire/plugin-subcommands": "^6.0.3",
    "@sapphire/plugin-utilities-store": "^2.0.2",
    "@sapphire/ratelimits": "^2.4.9",
    "@sapphire/shapeshift": "^3.9.6",
    "@sapphire/utilities": "^3.15.3",
    "@sentry/integrations": "^7.99.0",
    "@sentry/node": "^7.99.0",
    "@sentry/tracing": "^7.99.0",
    "@skyra/env-utilities": "^1.3.0",
    "@types/common-tags": "^1.8.4",
    "bullmq": "5.1.3",
    "colorette": "^2.0.20",
    "common-tags": "^1.8.2",
    "dayjs": "^1.11.10",
    "discord-api-types": "^0.37.67",
    "discord.js": "^14.14.1",
    "i18next": "^23.8.2",
    "prisma": "^5.9.1",
    "reflect-metadata": "^0.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@sapphire/cli": "^1.9.3",
    "@sapphire/eslint-config": "^5.0.3",
    "@sapphire/eslint-plugin-result": "^2.0.2",
    "@sapphire/ts-config": "^5.0.0",
    "@types/node": "^20.11.16",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.20.0",
    "@typescript-eslint/parser": "^6.20.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-tsdoc": "^0.2.17",
    "lint-staged": "^15.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.10",
    "prettier-plugin-prisma": "^5.0.0",
    "tsc-watch": "^6.0.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": "18.18.x",
    "npm": "^9.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BirthdayyBot/BirthdayyBotSapphire.git"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*": "prettier --ignore-unknown --write",
    "*.{mjs,js,ts}": "eslint --fix --ext mjs,js,ts"
  },
  "packageManager": "yarn@4.1.0",
  "volta": {
    "node": "18.19.0",
    "yarn": "4.1.0"
  }
}
